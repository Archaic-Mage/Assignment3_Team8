CHIP FPCircuit
{
    IN x[16],y[16],Bias[8];
    OUT z[16];
    PARTS:
    Xor(a=x[15],b=y[15],out=z[15]);
    
    PProduct(x[7]=true, x[0..6]=x[0..6], y[7]=true, y[0..6]=y[0..6], z[15]=P15 ,z[0..14]=ans);
    
    BarrelShifter16(P[15]=P15, P[0..14]=ans, P15=P15, Zm[0..6]=z[0..6]);
    
    Exponent(x[0..7]=x[7..14],y[0..7]=y[7..14],Bias[0..7]=Bias,P15=P15, z[0..7]=z[7..14]);

}